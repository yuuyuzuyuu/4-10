<div class="container">
  <div class="row">
    <div class="col-mid-3">
      <%= render 'users/info', user: current_user %>
    </div>
    <div class="col-mid-3">
      <%= render 'books/new', book: @book %>
    </div>

    <h2>Users</h2>
    <table class="users">
      <thead>
        <tr>
          <th>image</th>
          <th>name</th>
          <th>show</th>
        </tr>
      </thead>
      <tbody>
        <tr>
          <%= @users.each do |user| %>
          <th>image</th>
          <th><%= user.name %></th>
          <th>show</th>
        </tr>
      </tbody>
    </table>
    <% end %>
  </div>
</div>